{
    "name": "beporter/cakephp-env-awareness",
    "description": "Demo CakePHP 2 app that uses an environment variable to control configuration.",
    "homepage": "https://github.com/beporter/CakePHP-EnvAwareness",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~2.6.3",
        "cakephp/debug_kit": "~2.2",
        "cakedc/migrations": "~2.3",
        "loadsys/cakephp-config-read": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0"
    },
    "config": {
        "vendor-dir": "Vendor/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
